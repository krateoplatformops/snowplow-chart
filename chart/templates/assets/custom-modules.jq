def flipchar($c):
  {
    "a": "…ê", "b": "q", "c": "…î", "d": "p", "e": "«ù", "f": "…ü", "g": "∆É", "h": "…•", "i": "·¥â",
    "j": "…æ", "k": " û", "l": " É", "m": "…Ø", "n": "u", "o": "o", "p": "d", "q": "b", "r": "…π",
    "s": "s", "t": " á", "u": "n", "v": " å", "w": " ç", "x": "x", "y": " é", "z": "z",
    "A": "‚àÄ", "B": "êêí", "C": "∆Ü", "D": "p", "E": "∆é", "F": "‚Ñ≤", "G": "◊§", "H": "H",
    "I": "I", "J": "≈ø", "K": " û", "L": "À•", "M": "W", "N": "N", "O": "O", "P": "‘Ä",
    "Q": "Q", "R": "·¥ö", "S": "S", "T": "‚î¥", "U": "‚à©", "V": "Œõ", "W": "M", "X": "X",
    "Y": "‚ÖÑ", "Z": "Z", ".": "Àô", ",": "'", "'": ",", "\"": ",,", "_": "‚Äæ", "?": "¬ø",
    "!": "¬°", "(": ")", ")": "(", "[": "]", "]": "[", "{": "}", "}": "{"
  }[$c] // $c;

def flip($s):
  $s
  | explode
  | map([.] | implode | flipchar(.))
  | reverse
  | join("");